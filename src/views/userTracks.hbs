<section class="page">
  <h2> Tracks</h2>
  {{#each userContent}}
  <div class="user-card" id="card-{{id}}">
    <p class="user-card__text">{{title}}</p>
    <audio controls id="audio-{{id}}" ontimeupdate="this.currentTime >= {{target_time}} ? this.pause() : ''">
      <source src=" ../../assets/{{filename}}#t={{current_time_in_track}}" type="audio/mpeg">
      Your browser does not support the audio element.
    </audio>
  </div>
  {{/each}}
</section>